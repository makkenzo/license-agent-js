{
    "name": "@makkenzo/license-agent-js",
    "version": "0.1.0",
    "description": "Client library for validating licenses with the License Service API",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "license",
        "validation",
        "agent",
        "api"
    ],
    "author": "Daniyal Asylbekov <nekgo2009@gmail.com>",
    "license": "MIT",
    "packageManager": "pnpm@10.8.1",
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.14.1",
        "jest": "^29.7.0",
        "ts-jest": "^29.3.2",
        "tsup": "^8.4.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "axios": "^1.8.4",
        "ts-node": "^10.9.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
