        "agent",
        "api"
    ],
    "author": "Daniyal Asylbekov <nekgo2009@gmail.com>",
    "license": "MIT",
    "packageManager": "pnpm@10.8.1",
    "devDependencies": {
        "@types/node": "^22.14.1",
        "tsup": "^8.4.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "axios": "^1.8.4"
    },
    "publishConfig": {
        "access": "public"
    }
}
